@article{LokkevikEtAl1996,
  title = {Skin Treatment with Bepanthen Cream versus No Cream during Radiotherapy--a Randomized Controlled Trial},
  author = {L{\o}kkevik, E. and Skovlund, E. and Reitan, J. B. and Hannisdal, E. and Tanum, G.},
