@article{LatusEtAl2020,
  title = {{Kontrastmittelinduzierte akute Nierensch{\"a}digung -- Konsensuspapier der Arbeitsgemeinschaft ,,Herz -- Niere`` der Deutschen Gesellschaft f{\"u}r Kardiologie -- Herz- und Kreislaufforschung e.\,V. und der Deutschen Gesellschaft f{\"u}r Nephrologie e.\,V.}},
